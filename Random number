#include<studio.h>
#include<stdlib.h>
#include<conio.h>
Void main()
{
int a[500],b,i,j,temp,num[10],z;
clrscr();
rand();
for(i=1;i<=500;i++)
{
temp=Rand()%500;
for(j=1;j<=i-1;j++)
{
if(temp==a[j])
break;
}
if(i==j)
a[i]=temp;
else
i--;
}
for(i=1;i<=500;i++)
{
printf("%d",a[i]);
}
for(i=1;i<=10;I++)
{
for(j=i+1;j<=10;j++)
{
if(num[i]>num[j])
{
b=num[i];
num[i]=num[j];
num[j]=b;
z=rand()%10;
print ("\n %d",z);
}
}
}
printf("\n first tend sorted ascending random numbers:");
rand();
for(i=1;i<=10;i++)
{
printf("%d\n",i);
}
getch();
}

